{
  "name": "HoodMode",
  "version": "1.0.0",
  "description": "Bringing musical modality to ya hood",
  "main": "src/index.jsx",
  "scripts": {
    "bundle": "browserify -t babelify --debug src/index.jsx -o public/js/bundle.js",
    "deploy": "browserify -t babelify -t uglifyify src/index.jsx -o public/js/bundle.js ",
    "start": "node node_modules/.bin/budo src/index.jsx:bundle.js --dir=public --live --watch-glob=public/*.css --port=$PORT -- -t babelify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limikael/hoodmode.git"
  },
  "author": "limikael",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-preset-preact": "^2.0.0",
    "babelify": "^10.0.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "browserify": "^16.1.0",
    "budo": "^11.6.3",
    "jquery": "^3.4.1",
    "uglifyify": "^4.0.5",
    "@babel/polyfill": "^7.8.3",
    "bootstrap-icons": "^1.0.0-alpha2",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0"
  }
}
